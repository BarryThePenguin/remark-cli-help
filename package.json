{
  "name": "remark-cli-help",
  "version": "3.0.0",
  "description": "Add cli help output to a README with remark.",
  "license": "MIT",
  "keywords": [
    "text",
    "markdown",
    "plain",
    "text"
  ],
  "scripts": {
    "format": "remark *.md -qfo && xo --fix",
    "test-api": "ava",
    "test-coverage": "c8 ava",
    "test": "npm run format && npm run test-coverage",
    "test-watch": "ava -v -w"
  },
  "author": "Jonathan Haines <jonno.haines@gmail.com>",
  "contributors": [
    "Jonathan Haines <jonno.haines@gmail.com>"
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BarryThePenguin/remark-cli-help.git"
  },
  "bugs": {
    "url": "https://github.com/BarryThePenguin/remark-cli-help/issues"
  },
  "homepage": "https://github.com/BarryThePenguin/remark-cli-help#readme",
  "sideEffects": false,
  "type": "module",
  "exports": "./index.js",
  "files": [
    "lib/",
    "index.d.ts",
    "index.js"
  ],
  "devDependencies": {
    "ava": "^6.1.1",
    "c8": "^9.1.0",
    "is-hidden": "^1.1.1",
    "negate": "^1.0.0",
    "remark": "^14.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.1",
    "remark-usage": "^9.0.0",
    "vfile": "^5.0.0",
    "xo": "^0.56.0"
  },
  "dependencies": {
    "execa": "^5.0.0",
    "mdast-util-heading-range": "^2.1.2"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      "usage"
    ]
  },
  "xo": {
    "prettier": true,
    "esnext": false
  }
}
